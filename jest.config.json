{
  "verbose": true,
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "setupFiles": [
    "<rootDir>/setupTest.tsx"
  ],
  "moduleNameMapper": {
    "\\.(css|scss|less)$": "identity-obj-proxy"
  },
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "testMatch": [
    "**/*.test.(ts|tsx|js)",
    "**/__tests__/*.+(ts|tsx|js)"
  ]
}